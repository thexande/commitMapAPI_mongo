doctype
html( lang="en" ng-app="commitMap")
	head
		title commitMap Dash
		meta( charset='utf-8' )
		meta( http-equiv='X-UA-Compatible', content='IE=edge' )
		meta( name='viewport', content='width=device-width, initial-scale=1.0' )
		meta( name='description', content='Baking Bootstrap Snippets with Jade' )
		//- Bootswatch Theme
		link(href="//maxcdn.bootstrapcdn.com/bootswatch/3.3.0/cyborg/bootstrap.min.css", rel="stylesheet")
	//- dash override stylesheet
	link(href="/css/dash-overrides.css" rel="stylesheet")

	body( ng-app="commitMap" )
		// Fixed navbar
		.navbar.navbar-default.navbar-fixed-top(role='navigation')
			.navbar-header
				button.navbar-toggle(type='button', data-toggle='collapse', data-target='.navbar-collapse')
					span.sr-only Toggle navigation
					span.icon-bar
					span.icon-bar
					span.icon-bar
				a.navbar-brand(href='#') commitMap
				//- img(src='http://alexmurphy.me/img/git_map_pin_320x320.png')
			.navbar-collapse.collapse
				ul.nav.navbar-nav
					li
						a(ui-sref="dash.home") Dashboard
					li
						a(ui-sref='dash.repoSelect') Repo Watch
					li
						a(ui-sref="dash.activity") Activity Feed
		.container( ui-view="" )
